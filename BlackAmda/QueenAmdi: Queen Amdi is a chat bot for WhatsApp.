.pagination a:active, .pagination span:active, .pagination em:active { border-color: var(--color-border-secondary); transition: none 0s ease 0s; }

.pagination .previous_page, .pagination .next_page { color: var(--color-text-link); }

.pagination .current, .pagination .current:hover, .pagination [aria-current]:not([aria-current="false"]) { color: var(--color-state-selected-primary-text); background-color: var(--color-state-selected-primary-bg); border-color: transparent; }

.pagination .gap, .pagination .disabled, .pagination [aria-disabled="true"], .pagination .gap:hover, .pagination .disabled:hover, .pagination [aria-disabled="true"]:hover { color: var(--color-text-disabled); cursor: default; border-color: transparent; }

